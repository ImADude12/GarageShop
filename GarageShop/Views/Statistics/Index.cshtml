@{
    ViewData["Title"] = "Statistics";
}

<link rel="stylesheet" href="~/css/statistics.css" />
<div class="svg-container" style="margin-bottom: 15px;">
    <h1><u>Products each catagory</u></h1>
    <svg id="pie-svg" width="500" height="400" />
    <h1><u>Products each Seller</u></h1>
    <svg id="chart-svg" width="500" height="400"
</div>
<h1><u>Categories each seller</u></h1>



<table class="table table-dark">
    <thead>
  
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">1</th>
            <td>Mark</td>
            <td>Otto</td>

        </tr>
        <tr>
            <th scope="row">2</th>
            <td>Jacob</td>
            <td>Thornton</td>
        </tr>
        <tr>
            <th scope="row">3</th>
            <td>Larry</td>
            <td>the Bird</td>
        </tr>
    </tbody>
</table>


<h1><u>Tag each seller</u></h1>


<h1><u>product count each seller</u></h1>


<h1><u>average price each seller</u></h1>
@section Scripts{
    <script src="https://d3js.org/d3.v4.js"></script>
    <script src="~/js/statistics.js"></script>
}
